{% block publisher_form %}
    <div class="form-wrapper">
        {{ form_start(form) }}
            {{ form_errors(form) }}
            {{ form_row(form.address) }}
            {{ form_row(form.number) }}
            {% if form.url is defined %}
                {{ form_row(form.url) }}
            {% endif %}
            <div class="form-row">
                <button class="btn btn-primary" type="submit">save</button>
            </div>
        {{ form_end(form) }}
    </div>
{% endblock %}
