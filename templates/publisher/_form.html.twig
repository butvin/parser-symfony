{% block publisher_form %}
<div class="col-12">
<div class="form-wrapper">
{{ form_errors(form) }}
{{ form_start(form) }}
{{ form_row(form.address) }}
{{ form_row(form.number) }}
{% if form.url is defined %}
{{ form_row(form.url) }}
{% endif %}
<button class="btn btn-sm btn-primary" type="submit">
<span>save</span>
</button>
{{ form_end(form) }}
</div>
</div>
{% endblock %}
